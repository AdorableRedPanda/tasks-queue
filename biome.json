{
	"javascript": {
		"formatter": {
			"quoteStyle": "single"
		}
	},
	"linter": {
		"rules": {
			"style": {
				"noEnum": "error"
			},
			"complexity": {
				"noForEach": "off",
				"noExcessiveLinesPerFunction": {
					"level": "error",
					"options": {
						"maxLines": 40
					}
				}
			},
			"correctness": {
				"noUnusedImports": "error",
				"useExhaustiveDependencies": "off"
			}
		}
	}
}
